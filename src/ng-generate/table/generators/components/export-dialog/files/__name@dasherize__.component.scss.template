  justify-content: space-between;
  display: flex;
  align-items: center;
  margin-top: 1rem;
  margin-bottom: 1rem;
  font-weight: bold;
}

.close-dialog-icon {
  background: none;
  border: none;
  font-size: 1.3rem;
  cursor: pointer;
  margin-left: auto;
}

