<form [formGroup]="form" (ngSubmit)="onSubmit()" class="<%= options.name %>">
    <% for(let fieldConfig of options.fieldsConfigs) { %>
        <%= options.include(fieldConfig.htmlTemplatePath, { fieldConfig: fieldConfig, include: options.include }) %>
    <% } %>

    <section class="form-actions">
        <button mat-flat-button (click)="onCancel()">Cancel</button>
        <button mat-flat-button color="primary" [disabled]="form.invalid">Submit</button>
    </section>
</form>
